          Routine:
        </td>
        <td colspan='3'>
          Plan
        </td>
        <td colspan='4'>
          Actual
        </td>

        <set_h>Set</set_h>
        <exercise_h>Exercise</exercise_h>
        <lbs_h>lbs</lbs_h>
        <reps_h>reps</reps_h>
        <rest_h>rest</rest_h>
        <effort_h>effort</effort_h>
        <lbs_h>lbs</lbs_h>
        <reps_h>reps</reps_h>
        <rest_h>rest</rest_h>

        <set rowspan='2'>1</set>
        <exercise>benchpress</exercise>
        <lbs>120</lbs>
        <reps>5</reps>
        <rest rowspan='2'>1:30</rest>
        <effort>hard</effort>
        <lbs>100</lbs>
        <reps>1</reps>
        <rest rowspan='2'>3:00</rest>

        <exercise>curl</exercise>
        <lbs>50</lbs>
        <reps>5</reps>
        <effort></effort>
        <lbs>50</lbs>
        <reps>2</reps>
